<h1>Convert <%=@original_currency %></h1>

<%#= @parsed_data.keys%>
<p>Go <a href="/">back</a></p>

<ul>
  <% @currencies.keys.each do |symbol| %>
    <li>
      <a href="/<%=@original_currency %>/<%= symbol %>">Convert 1 <%=@original_currency %> to <%= symbol %>...</a>
    </li>

  <% end %>
</ul>
